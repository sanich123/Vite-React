    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.0",
    "lint-staged": "^12.4.1",
    "msw": "^1.1.0",
    "nodemon": "^2.0.22",
