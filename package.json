    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
